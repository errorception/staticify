{
  "name": "staticify",
  "version": "1.0.0",
  "description": "A better static asset handler for node.js",
  "main": "index.js",
  "author": "Rakesh Pai <rakeshpai@errorception.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/errorception/staticify.git"
  },
  "scripts": {
    "mocha": "mocha --reporter spec",
    "xo": "xo",
    "test": "npm run xo && npm run mocha"
  },
  "keywords": [
    "static",
    "asset",
    "versioning",
    "caching",
    "express",
    "expressjs"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "send": "~0.16.1"
  },
  "devDependencies": {
    "mocha": "~4.0.1",
    "should": "~13.1.3",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": 4,
    "rules": {
      "capitalized-comments": "off",
      "spaced-comment": "off",
      "func-style": [
        "error",
        "expression"
      ]
    },
    "overrides": [
      {
        "files": "test/**",
        "envs": [
          "mocha",
          "node"
        ]
      }
    ]
  },
  "engines": {
    "node": ">=4"
  }
}
